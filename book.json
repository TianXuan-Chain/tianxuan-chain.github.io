{
  "title": "天玄技术文档",
  "language": "zh-hans",
  "author": "Emosama",
  "theme-default": {
    "showLevel": true
  },
  "plugins": [
    "splitter",
    "hide-element",
    "search-pro-fixed",
    "-highlight",
    "chapter-fold",
    "expandable-chapters",
    "tbfed-pagefooter",
    "github",
    "flexible-alerts",
    "prism",
    "auto-scroll-table",
    "theme-d4t",
    "-search",
    "-lunr",
    "image-captions",
    "-sharing",
    "code",
    "back-to-top-button",
    "pageview-count"
  ],
  "variables": {
    "d4t": {
      "nav": [
        {
          "url": "https://github.com/TianXuan-Chain",
          "target": "_blank",
          "name": "天玄代码仓库"
        }
      ]
    }
  },
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/TianXuan-Chain"
    },
    "hide-element": {
      "elements": [
        ".gitbook-link",
        ".title",
        ".treeview__copyright",
        ".treeview__main-title"
      ]
    },
    "d4t": {
      "search-placeholder": "请输关键字搜索",
      "logo": "./assets/logo.svg",
      "favicon": "./assets/fav.ico"
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy netease 2024",
      "modify_label": "文档修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "flexible-alerts": {
      "style": "callout",
      "comment": {
        "label": "Comment",
        "icon": "fa fa-comments",
        "className": "info"
      }
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ],
      "lang": {
        "flow": "typescript"
      },
      "ignore": [
        "mermaid",
        "eval-js",
        "ascii",
        "result",
        "manifest",
        "payload",
        "google",
        "tree",
        "java_out",
        "log4j2",
        "jsp",
        "class",
        "Exception",
        "stack",
        "c#",
        "xml-dtd",
        "C++",
        "twig",
        "jinja2",
        "exception",
        "tpl"
       ]
    },
    "fontsettings": {
      "theme": "white",
      "family": "sans",
      "size": 2
    },
    "image-captions": {
      "caption": "Image - _CAPTION_"
    },
    "code": {
      "copyButtons": true
    }
  },
  "structure": {
    "readme": "overview.md"
  }
}
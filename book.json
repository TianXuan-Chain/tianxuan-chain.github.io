{
  "title": "天玄技术文档",
  "language": "zh-hans",
  "author": "Emosama",
  "plugins": [
    "theme-fexa",
    "codetabs",
    "-search",
    "favicon",
    "-lunr",
    "search-plus",
    "atoc",
    "image-captions",
    "-sharing",
    "-livereload",
    "code",
    "cuav-chapters",
    "heading-anchors",
    "back-to-top-button",
    "pageview-count"
  ],
  "variables": {
    "themeFexa":{
        "nav":[
            {
                "url":"https://github.com/TianXuan-Chain",
                "target":"_blank",
                "name": "天玄代码库"
            }
        ]
    }
  },
  "pluginsConfig": { 
    "fontsettings": {
      "theme": "white", 
      "family": "sans", 
      "size": 2
    },
    "image-captions": {
      "caption": "Image - _CAPTION_"
    },
    "code": {
      "copyButtons": true
    },
    "favicon": {
      "shortcut": "assets/logo.jpg",
      "bookmark": "assets/logo-32.ico"
    },
    "atoc": {
      "addClass": true,
      "className": "atoc"
    },
    "edit-link": {
      "base": "https://github.com/TianXuan-Chain/thanos-docs",
      "label": "Edit This Page"
    },
    "theme-fexa":{
      "search-placeholder":"输入关键字搜索",
      "logo":"assets/logo-120.ico", 
      "favicon": "assets/logo-32.ico"
    }
  },
  "structure": {
    "readme": "overview.md"
  }
}
